{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "attach",
        "name": "Attach to WebClient",
        "port": 9222,
        "webRoot": "${workspaceFolder}/src/WebClient/src",
        "sourceMapPathOverrides": {
          "webpack:///./src/*": "${webRoot}/*"
        }
      },
      {
        "type": "chrome",
        "request": "attach",
        "name": "Attach to Single Source",
        "port": 9222,
        "webRoot": "${workspaceFolder}/src/WebClient/public",
      }
    ]
}
